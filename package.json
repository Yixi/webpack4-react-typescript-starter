{
  "version": "0.0.1",
  "author": "yixi",
  "scripts": {
    "start": "yarn dev",
    "dev": "webpack serve --config build/webpack.dev.js --progress",
    "build": "webpack --config build/webpack.build.js --progress",
    "test": "jest",
    "lint": "yarn tslint --project tsconfig.json",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn typecheck && yarn test"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,json,md}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "swr": "^0.3.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "fork-ts-checker-webpack-plugin": "^6.0.4",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
